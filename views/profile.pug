extends layout

block content
    .container
        .top
            h3 Twitter
        .row
            .column
                p id:
                p token:
                p username:
                p displayName:
            .column
                p= user.twitter.id
                p= user.twitter.token
                p= user.twitter.username
                p= user.twitter.displayName
        .row
            .column
                h3 You are attending .
                each key in attend
                    .row
                        .column
                            h5= key.barId
                        .column
                            button.going(id=key.barId) Cancel visit
        
